(defun vecop1 (_)
	;trashes
	;r1-r6+
	(entry 'nums _ '(r0 r1 r2))
	(class/array/get_both r0 r3 r4)
	(assign '((r1 array_begin) (r2 array_begin)) '(r1 r2))
	(vec-set 5)
	(loop-start)
		(vec-load-long 1 r1)
		(vec-load-long 1 r2)
		((eval (sym (cat "vec-" _))) 1)
		(vec-store-long 1 r3)
		(vp-add-cr long_size r3)
		(vp-add-cr long_size r1)
		(vp-add-cr long_size r2)
	(loop-until '(r3 = r4))
	(exit 'nums _ '(r0))
	(vp-ret))

(defun vecop2 (_)

	(class/array/get_both r0 r7 r8)
	(assign '((r1 array_begin) (r2 array_begin)) '(r5 r6))
	(vec-set 0)
	(loop-start)
		(vec-load-long 1 r5)
		(vec-load-long 1 r6)
		((eval (sym (cat "vec-" _))) 1)
		(vec-store-long 1 r7)
		(vp-add-cr long_size r7)
		(vp-add-cr long_size r5)
		(vp-add-cr long_size r6)
	(loop-until '(r7 = r8))
	(exit 'nums _ '(r4))
	(vp-ret))


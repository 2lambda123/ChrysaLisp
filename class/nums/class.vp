(def-method 'nums 'sum)
	;inputs
	;r0 = nums object (ptr)
	;outputs
	;r0 = nums object (ptr)
	;r1 = sum (long)
	;trashes
	;r1-r4

	(entry 'nums 'sum '(r0))

	(call 'nums 'get_both '(r0) '(r0 r1 r2))
	(vp-xor-rr r3 r3)
	(loop-while '(r1 /= r2))
		(assign '((r1 0)) '(r4))
		(vp-add-cr long_size r1)
		(vp-add-rr r4 r3)
	(loop-end)

	(exit 'nums 'sum '(r0 r3))
	(vp-ret)

(def-func-end)


(include 'sys/func.inc)
(include 'class/lisp/class.inc)

(def-method 'lisp 'func_progn)
	;inputs
	;r0 = lisp object (ptr)
	;r1 = args vector object (ptr)
	;outputs
	;r0 = lisp object (ptr)
	;r1 = return value object (ptr)
	;trashes
	;all but r0

	(entry 'lisp 'func_progn '(r0 r1))

	(class/array/get_length r1 r2)
	(vpif '(r2 == 0))
		(vp-cpy-ir r0 lisp_sym_nil r1)
	(else)
		(class/array/get_begin r1 r1)
		(vp-shl-cr (log2 long_size) r2)
		(vp-add-rr r1 r2)
		(vp-cpy-ir r2 (neg ptr_size) r1)
	(endif)
	(class/obj/ref r1 r2)

	(exit 'lisp 'func_progn '(r0 r1))
	(vp-ret)

(def-func-end)
